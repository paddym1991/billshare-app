{{#each groups}}
    <section class="ui stacked segment">
        <h2 class="ui header">
            {{title}}
        </h2>
        <p> Members: {{total}} </p>
        <a href="/group/{{id}}" class="ui icon button">
            <i class="icon folder open"></i>
        </a>
        <a href="/dashboard/deletegroup/{{id}}" class="ui icon button">
            <i class="icon trash"></i>
        </a>
    </section>
{{/each}}